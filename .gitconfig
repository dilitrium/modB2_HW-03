[user]
        name = dilitrium
        email = dilitrium@gmail.com
[core]
        autocrlf = input
        safecrlf = warn
        quotepath = off
[alias]
        history = log
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p

